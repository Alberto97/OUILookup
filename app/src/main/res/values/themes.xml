<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.OUILookup" parent="Theme.AppCompat.DayNight">
        <item name="colorPrimary">@color/blue_500</item>
        <item name="colorPrimaryDark">@color/blue_500</item>
    </style>

    <style name="Theme.OUILookup.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.OUILookup.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.OUILookup.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>